<?php $pagePar = $this->page ? $this->page : 'page'; ?><?php if ($this->pageCount > 1) { ?>	<nav>		<ul class="pagination"> 			<?php if ($this->firstPageInRange > 1)	{ ?>				<li class="first"><a href="<?php echo $this->customUrl('', array($pagePar => 1)); ?>">&laquo; First</a></li>			<?php } ?>						<?php if (isset($this->previous))	{ ?>				<li class="prev"><a href="<?php echo $this->customUrl('', array($pagePar => $this->previous)); ?>">&lt; Prev</a></li>			<?php } ?>						<?php foreach ($this->pagesInRange as $page) { ?>				<li <?php echo $page == $this->current ? 'class="active"' : '' ?>>					<a href="<?php echo $this->customUrl('', array($pagePar => $page)); ?>">						<?php echo $page; ?>					</a>				</li>			<?php } ?>						<?php if (isset($this->next))	{ ?>				<li class="next"><a href="<?php echo $this->customUrl('', array($pagePar => $this->next)); ?>">Next &gt;</a></li>			<?php } ?>						<?php if ($this->lastPageInRange < $this->pageCount)	{ ?>				<li class="last"><a href="<?php echo $this->customUrl('', array($pagePar => $this->last)); ?>">Last &raquo;</a></li>			<?php } ?>		</ul>	</nav><?php } ?>